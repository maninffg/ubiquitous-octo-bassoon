-- 获取 StarterGui 服务
local StarterGui = game:GetService("StarterGui")

-- 显示"请稍等"通知
StarterGui:SetCore("SendNotification", {
    Title = "加载中",
    Text = "请稍等...",
    Icon = "rbxassetid://75702897877244", -- 您提供的图标ID
    Duration = 5, -- 显示5秒
})

-- 等待1秒，确保通知被看到
wait(5)

-- 加载主脚本
--《全部都要复制 包括这句话》
HUB = "脚本中心"
Script = "通用脚本"
loadstring(game:HttpGet("https://raw.githubusercontent.com/maninffg/-/refs/heads/main/main.lua"))()
